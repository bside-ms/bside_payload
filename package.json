{
  "name": "bside-payload",
  "description": "B-Side Website Backend",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "packageManager": "yarn@4.9.2",
  "scripts": {
    "ts-check": "tsc --noEmit --pretty",
    "ts-check:watch": "yarn ts-check --watch --preserveWatchOutput",
    "dev": "next dev",
    "build": "next build",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate:importmap": "payload generate:importmap",
    "generate:types": "payload generate:types",
    "lint": "next lint",
    "payload": "payload",
    "start": "next start",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "^3.52.0",
    "@payloadcms/next": "^3.52.0",
    "@payloadcms/plugin-nested-docs": "^3.52.0",
    "@payloadcms/plugin-redirects": "^3.52.0",
    "@payloadcms/plugin-seo": "^3.52.0",
    "@payloadcms/richtext-slate": "^3.52.0",
    "@payloadcms/ui": "^3.52.0",
    "axios": "^1.11.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "graphql": "^16.11.0",
    "lodash-es": "^4.17.21",
    "next": "^15.4.6",
    "passport": "^0.6",
    "payload": "^3.52.0",
    "payload-blurhash-plugin": "^3.1.0",
    "payload-oauth2": "^1.0.19",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sharp": "0.34.2",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/lodash-es": "^4",
    "@types/node": "^24.3.0",
    "@types/passport": "^1.0.17",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.33.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1"
  }
}
